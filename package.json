{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "/hooks/",
    "/screens/",
    "/components/",
    "^../(.*)",
    "^./(.*)",
    "/module.scss/"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "devDependencies": {
    "prettier": "^3.0.3"
  }
}
